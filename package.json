{
  "name": "gas-md2html",
  "version": "0.4.0",
  "description": "Google Apps Script library to convert Markdonw to HTML",
  "author": "",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git://github.com/<user>/<repository>.git"
  },
  "bugs": {
    "url": "https://github.com/<user>/<repository>/issues"
  },
  "keywords": [
    "gas",
    "markdown",
    "html"
  ],
  "main": "index.js",
  "type": "module",
  "scripts": {
    "setup:clasp": "bash scripts/setup-clasp.sh && bash scripts/setup-clasprc.sh",
    "cleanup:clasp": "bash scripts/cleanup-clasp.sh",
    "clean": "rimraf build/*",
    "build": "npm run clean && rollup --config && cat scripts/banner.txt build/main.js src/index.js > build/md2html.js && cp LICENSE build/LICENSE.txt && rimraf build/main.js build/src build/test",
    "push": "bash scripts/clasp-with-auth.sh push",
    "deploy": "bash scripts/clasp-with-auth.sh deploy",
    "test": "node --experimental-vm-modules node_modules/.bin/jest"
  },
  "dependencies": {
    "hast-util-to-html": "^8.0.3",
    "mdast-util-from-markdown": "^1.2.0",
    "mdast-util-to-hast": "^12.1.0"
  },
  "devDependencies": {
    "@rollup/plugin-commonjs": "^21.0.1",
    "@rollup/plugin-node-resolve": "^13.1.3",
    "@rollup/plugin-typescript": "^8.3.0",
    "@types/google-apps-script": "^1.0.44",
    "@types/jest": "^27.4.0",
    "jest": "^27.4.7",
    "rimraf": "^3.0.2",
    "rollup": "^2.64.0",
    "rollup-plugin-license": "^2.6.1",
    "ts-jest": "^27.1.3",
    "ts-node": "^10.4.0",
    "tslib": "^2.3.1",
    "typescript": "^4.5.4"
  }
}
